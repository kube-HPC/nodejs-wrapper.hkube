{
  "name": "@hkube/nodejs-wrapper",
  "version": "1.0.3",
  "description": "nodejs wrapper for hkube",
  "main": "index.js",
  "scripts": {
    "lint": "./node_modules/eslint/bin/eslint.js .",
    "test": "NODE_ENV=test ./node_modules/mocha/bin/_mocha tests --colors --recursive --exit",
    "test:cov": "NODE_ENV=test ./node_modules/nyc/bin/nyc.js --exclude=tests --reporter=html --reporter=text --reporter=lcov mocha tests --colors --recursive --exit",
    "coverage": "cat ./coverage/lcov.info | ./node_modules/coveralls/bin/coveralls.js",
    "build": "npm publish"
  },
  "pre-commit": {
    "run": [
      "lint",
      "test"
    ]
  },
  "author": "Hkube",
  "license": "MIT",
  "dependencies": {
    "ws": "^7.0.0"
  },
  "devDependencies": {
    "chai": "^4.2.0",
    "coveralls": "^3.0.3",
    "eslint": "^5.16.0",
    "eslint-config-airbnb-base": "^13.1.0",
    "eslint-plugin-chai-friendly": "^0.4.1",
    "eslint-plugin-import": "^2.16.0",
    "lodash.orderby": "^4.6.0",
    "mocha": "^6.0.2",
    "mockery": "^2.1.0",
    "nyc": "^13.3.0",
    "pre-commit": "^1.2.2",
    "sinon": "^7.3.1"
  }
}
